{
  "name": "wicadu-desing-web",
  "version": "0.0.1",
  "description": "UI library to sistem desing wicadu",
  "main": "index.js",
  "files": [
    "atoms",
    "molecules",
    "functions",
    "styles"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wicadu/wicadu-design-jsx"
  },
  "scripts": {
    "build:babel": "babel dist -d dist && mv dist/* ./",
    "rmFolders": "rm -rf dist && rm -rf atoms && rm -rf molecules",
    "build:ts": "npm run rmFolders && tsc -p tsconfig.json",
    "build": "npm run build:ts && npm run build:babel",
    "build:storybook": "build-storybook -o ./build"
  },
  "author": "Wicadu",
  "license": "ISC",
  "homepage": "https://github.com/wicadu/wicadu-design-jsx/blob/master/README.md",
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "styled-components": "^5.2.0",
    "polished": "^4.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/types": "^7.12.0",
    "@sambego/storybook-state": "^2.0.1",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/react": "^16.9.52",
    "babel-loader": "^8.1.0",
    "typescript": "^4.0.3"
  }
}
